@model AgentsListViewModel

<h1>@Model.CurrentAgency</h1>
@foreach (var agent in Model.agents)
{
    <div class="col-sm-3 col-lg-3 col-md-3">
        <div class="thumbnail">
            <img src="@agent.ImageThumbnailUrl" alt="">
            <div class="caption">

                <h3>
                    <a>@agent.Name</a>
                </h3>
                <p><i style="font-size:18px" class="fa">&#xf095;</i> &nbsp;  @agent.ContactNumber</p>
                <p>

                    @for (int i = 0; i < (@agent.Rating); i++)
                    {
                        <span class="fa fa-star checked"></span>
                    }

                    @agent.Rating Stars | @agent.NumberOfReviews.ToString() Reviews
                </p>

                <p style="background-color:lightgray">@agent.LongDescription</p>
            </div>
        </div>
    </div>
}

